# Database Configuration
# PostgreSQL connection string
# Format: postgresql://username:password@localhost:5432/database_name
DATABASE_URL=postgresql://postgres:password@localhost:5432/logis_webflow

# Server Configuration
PORT=5000
NODE_ENV=development

# Session Secret
# Generate a random string for production (e.g., using: openssl rand -base64 32)
SESSION_SECRET=your-secret-key-change-in-production

# Admin User Seeding (Optional - defaults provided)
# These are used to create the initial admin user on first run
ADMIN_EMAIL=admin@logistics.com
ADMIN_PASSWORD=admin123
ADMIN_NAME=System Admin
